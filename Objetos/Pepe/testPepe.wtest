import pepe.*

test "Cuando pepe es desarrollador y tiene bono presentismo nulo"{
  pepe.puesto(desarrollador)
  pepe.bonoPresentismo(bonoPresentismoNulo)
  pepe.bonoResultado(bonoResultadoFijo)
  assert.equals(1041, pepe.sueldo())
}

test "Cuando pepe es desarrollador y tiene bono presentismo faltas"{
  pepe.faltas(0)
  pepe.puesto(desarrollador)
  pepe.bonoPresentismo(bonoPresentismoFaltas)
  pepe.bonoResultado(bonoResultadoFijo)
  assert.equals(1141, pepe.sueldo())
}